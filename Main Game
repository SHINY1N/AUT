local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Shiny GUI - AUT", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


--Bool Values
_G.Shiny = false
_G.AutoQuest = false

--functions
function autoclick()
	while _G.Shiny == true do wait()
		-- Script generated by SimpleSpy - credits to exx#9394

		local args = {
			[1] = "MouseButton1"
		}

		game:GetService("ReplicatedStorage").ReplicatedModules.KnitPackage.Knit.Services.MoveInputService.RF.FireInput:InvokeServer(unpack(args))
	end
end

function autoquest()
	while _G.AutoQuest == true do wait()
		local args = {
			[1] = "Sakuya"
		}

		game:GetService("ReplicatedStorage").ReplicatedModules.KnitPackage.Knit.Services.DialogueService.RF.CheckDialogue:InvokeServer(unpack(args))
	end
end

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Useful things ig"
})

Tab:AddToggle({
	Name = "AutoClick",
	Default = false,
	Callback = function(Value)
		_G.Shiny = Value
		autoclick()
	end    
})

Tab:AddToggle({
	Name = "Auto Sakuya Quest",
	Default = false,
	Callback = function(Value)
		_G.AutoQuest = Value
		autoquest()
	end    
})
Tab:AddButton({
	Name = "Take Daily Quest",
	Callback = function()
		local args = {
			[1] = "Daily Quest"
		}

		game:GetService("ReplicatedStorage"):WaitForChild("ReplicatedModules"):WaitForChild("KnitPackage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DialogueService"):WaitForChild("RF"):WaitForChild("CheckDialogue"):InvokeServer(unpack(args))

	end    
})

Tab:AddButton({
	Name = "Anti Afk",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/SHINY1N/Anti_AFK/main/ShinyBest'))()
	end    
})
Tab:AddButton({
	Name = "Load Death GUI [L = Toggle] (Important for Autofarm)",
	Callback = function()
		_G.Options = {

			["ToggleKey"] = "L",

		}

		loadstring(game:HttpGet(("https://raw.githubusercontent.com/VoidGaster/Scripts/main/" .. tostring(game.PlaceId)),true))()

		--iExotixPloitz
	end    
})

local Tab = Window:MakeTab({
	Name = "Info for Sayu",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddLabel("*German... JUHUUU*")

Tab:AddParagraph("Info for Sayu","Öffne `Death GUI` dann geh zu Misc, und schalte (wenn du reaper benutzt, was das beste wäre) Autofarm Thugs, und Auto Move: Y,T,R an. sowie den autoclick und auto sakuya quest von meinem script, und dazu einmal auf anti afk clicken c:")

local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddLabel("Script is Made by SHINY N, Death GUI Credit to iExotixPloitz")
OrionLib:Init()
